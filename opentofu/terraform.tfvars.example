# Exemplo de preenchimento; copie para terraform.tfvars e ajuste para seu ambiente
container_name    = "node-red"
host_port         = 1880
data_dir_name     = "node-red"
data_dir_base     = "/DATA/AppData" # change to a user-writable path
mongo_network_name       = "mongo-node-red"
existing_mongo_container = "mongodb"
mongo_username           = "node_red_user"
mongo_password           = "change-me"
mongo_database           = "node_red_db"
mongo_auth_username      = ""
mongo_auth_password      = ""
mongo_auth_db            = "admin"
node_red_image    = "nodered/node-red:4.1.2-22"
custom_image_name = "node-red-custom:latest"
remove_volumes_on_destroy = false
